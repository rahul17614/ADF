{
	"name": "AzureBlobOutput",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureBlobStorage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"filename": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().filename",
					"type": "Expression"
				},
				"folderPath": "sqltable",
				"container": "adfoutput"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "Invoice_id",
				"type": "String"
			},
			{
				"name": "Cust_id",
				"type": "String"
			},
			{
				"name": "Year",
				"type": "String"
			},
			{
				"name": "Product_id",
				"type": "String"
			},
			{
				"name": "Product",
				"type": "String"
			},
			{
				"name": "Sales",
				"type": "String"
			}
		]
	}
}